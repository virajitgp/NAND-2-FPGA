#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib64/ivl/system.vpi";
:vpi_module "/usr/lib64/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib64/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib64/ivl/v2005_math.vpi";
:vpi_module "/usr/lib64/ivl/va_math.vpi";
S_0x558632d95530 .scope module, "test" "test" 2 1;
 .timescale 0 0;
    .scope S_0x558632d95530;
T_0 ;
    %vpi_call 2 3 "$dumpfile", "test.vcd" {0 0 0};
    %vpi_call 2 4 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x558632d95530 {0 0 0};
    %vpi_call 2 5 "$display", "Hello, Icarus Verilog!" {0 0 0};
    %delay 10, 0;
    %vpi_call 2 6 "$finish" {0 0 0};
    %end;
    .thread T_0;
# The file index is used to find the file name in the following table.
:file_names 3;
    "N/A";
    "<interactive>";
    "test.v";
